
.ms-addressItem {
  background: #fff;
}
.addressWarp {
  display: flex;
  font-size: 28rpx;
  padding: 30rpx 30rpx 24rpx 0;
  border-bottom: 1px solid #EDEDED;
  line-height: 44rpx;
}
.addressWarp .infoWarp {
  flex: 2;
  display: flex;
}
.ms-addressItem .name {
  width: 180rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction:column;
}
.ms-addressItem .name view{
  text-overflow: ellipsis;
  overflow: hidden;
  width: 160rpx;
  white-space: nowrap;
}
.ms-addressItem .name .tag {
  color: #e2b260;
  border-radius: 4rpx;
  border:1px solid #e2b260;
  text-align: center;
  font-size: 20rpx;
  height: 28rpx;
  line-height: 30rpx;
  padding: 0 8rpx;
}
.ms-addressItem .info {
  flex: 2;
}
.ms-addressItem .info .address {
  color: #7f7f7f;
  font-size: 24rpx;
  line-height: 28rpx;

}
.ms-addressItem .detele {
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left:20rpx;
}
.ms-addressItem .detele image {
  width: 34rpx;
  height: 34rpx;
}


