/**buying.wxss**/

.ms-fixed {
    position: fixed;
    left: 0;
    width: 100%;
    z-index: 100;
}

.ms-container-buying {
    background-color: #fff;
    height: 100%;
}

.ms-banner .image {
    width: 100%;
    vertical-align: top;
}

.buyNav {
    display: flex;
    flex-direction: row;
    white-space: nowrap;
    position: relative;
    width: 100%;
    background-color: #fff;
}

.buyNav .con {
    position: relative;
    display: inline-block;
    box-sizing: border-box;
    padding-bottom: 20rpx;
    z-index: 2;
    flex: 1;
}

.buyNav .item:last-child:after {
    display: none;
}

.buyNav .item .view {
    background-color: #fbe9ea;
    display: block;
    padding: 16rpx 0;
    position: relative;
    z-index: 2;
    width: 100%;
    overflow: hidden;
}

.buyNav .item .text {
    display: block;
    text-align: center;
}

.buyNav .item .text.date {
    font-size: 28rpx;
    height: 40rpx;
}

.buyNav .item .text.state {
    font-size: 24rpx;
    height: 34rpx;
    color: #9a9a9a;
}

.buyNav .item .view::after {
    display: block;
    content: "";
    position: absolute;
    top: 10%;
    right: 0;
    height: 80%;
    border-right: 1px solid #fbd0d1;
}

.ms-isFixed {
    background-color: #fff;
    height: 126rpx;
}

.buyNav .item.cur::after {
    position: absolute;
    display: block;
    content: "";
    height: 24rpx;
    width: 24rpx;
    background: #E72722;
    margin-left: -12rpx;
    left: 50%;
    bottom: 8rpx;
    transform: rotate(45deg);
    z-index: 1;
}
.buyNav .con .item {
    padding: 10rpx 0;
    background-color: #f9e9ea;
}
.buyNav .con.cur .item {
    background-color: #E72722;
}

.buyNav .con.cur .item::after {
    display: none;
}

.buyNav .con.cur .item .text {
    color: #fff;
}

.ms-buyStatus {
    background-color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    padding-bottom: 10rpx;
}

.ms-buyStatus::before {
    content: "";
    display: block;
    width: 60%;
    border-top: 1px solid #333;
    position: absolute;
    z-index: 1;
}

.ms-buyStatus .item {
    position: relative;
    padding: 30rpx;
    background-color: #fff;
    font-size: 28rpx;
    z-index: 2;
}

.ms-buyListItem .item {
    box-sizing: border-box;
    padding: 18rpx;
    display: flex;
    white-space: nowrap;
    background-color: #fff;
    margin-top: 15rpx;
}

.ms-buyListItem .item .view {
    display: inline-block;
}

.ms-buyListItem .item .pic {
    margin-right: 18rpx;
    position: relative;
    width: 270rpx;
}

.ms-buyListItem .item .pic .image {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
}

.ms-buyListItem .item .pic .image.icon {
    display: none;
    width: 50%;
    height: 50%;
    top: 25%;
    left: 25%;
}

.ms-buyListItem .item .pic::after {
    display: block;
    content: "";
    width: 100%;
    padding-bottom: 100%;
}

.ms-buyListItem .item .info {
    box-sizing: border-box;
    padding-top: 20rpx;
    overflow: hidden;
    flex-direction: column;
    display: flex;
    width: 100%;
    flex: 1;
    position: relative;
}

.ms-buyListItem .item .info .text,
.ms-buyListItem .item .info .navigator {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.ms-buyListItem .item .info .title {
    font-size: 32rpx;
}

.ms-buyListItem .item .info .brief {
    margin-top: 10rpx;
    font-size: 24rpx;
    color: #7f7f7f;
}

.ms-buyListItem .item .info .price {
    padding: 10rpx 0;
}

.ms-buyListItem .item .info .price .view {
    font-size: 24rpx;
    margin-right: 20rpx;
    color: #7f7f7f;
}

.ms-buyListItem .item .info .price .view .text {
    color: #E72722;
    font-size: 30rpx;
}

.ms-buyListItem .item .info .price .view.del {
    text-decoration: line-through;
}

.ms-buyListItem .item .info .progress {
    width: 200rpx;
    height: 34rpx;
    border: 1px solid #E72722;
    border-radius: 30rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    overflow: hidden;
    z-index: 1;
}

.ms-buyListItem .item .info .bottom {
    margin-top: 10rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    white-space: nowrap;
}

.ms-buyListItem .item .info .progress .view {
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    background-color: #fbe9ea;
}

.ms-buyListItem .item .info .progress .text {
    font-size: 24rpx;
    color: #E72722;
    position: relative;
    z-index: 2;
}

.ms-buyListItem .item .info .text {
    color: #E72722;
    font-size: 32rpx;
}

.ms-buyListItem .item .info .bottom .label {
    display: none;
    font-size: 24rpx;
    width: 135rpx;
    height: 50rpx;
    background-color: #E72722;
    color: #fff;
    line-height: 50rpx;
    text-align: center;
}

.ms-buyListItem .item.notice .info .bottom .label.notice,
.ms-buyListItem .item.over .info .bottom .label.over,
.ms-buyListItem .item.buy .info .bottom .label.buy {
    display: inline-block;
}

.ms-buyListItem .item.over .info .bottom .label.buy,
.ms-buyListItem .item.notice .info .bottom .label.over {
    display: none;
}

.ms-buyListItem .item.over .pic .image.icon {
    display: block;
}

.default_card {
    position: relative;
}

.default_card .card {
    width: 100%;
    position: relative;
}

.default_card .e_code {
    position: absolute;
    width: 160rpx;
    height: 170rpx;
    background: transparent;
    right: 40rpx;
    top: 26rpx;
}

.default_card .num_pack {
    position: absolute;
    bottom: 120rpx;
    left: 60rpx;
    display: flex;
    flex-direction: row;
    height: 50rpx;
    line-height: 50rpx;
}

.default_card .num_pack .text {
    color: #fff;
}

.default_card .num_pack .hdCardNum {
    color: #fff;
    font-weight: 600;
    font-size: 46rpx;
    letter-spacing: 1px;
    padding-right: 12rpx;
}

.default_card .num_pack .packbag {
    font-size: 24rpx;
    height: 40rpx;
    color: #fde9c2;
    border: 1px solid #fde9c2;
    border-radius: 28rpx;
    padding: 0rpx 24rpx 0 18rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.default_card .classify {
    width: 100%;
    position: absolute;
    bottom: 54rpx;
    left: 0rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 0 60rpx;
    box-sizing: border-box;
}

.default_card .classify .navigator,
.like_navigator {
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 24rpx;
    color: #fff;
    position: relative;
}

.default_card .classify .navigator::after,
.like_navigator::after {
    content: '';
    height: 28rpx;
    width: 2rpx;
    position: absolute;
    left: -16rpx;
    top: 4rpx;
    background: #fff;
    opacity: 0.5;
}

.default_card .classify .navigator:first-child::after,
.like_navigator:first-child::after {
    display: none;
}

.default_card .classify .image {
    width: 32rpx;
    height: 32rpx;
    padding: 0 6rpx 0 0;
}

.qrcode-up {
