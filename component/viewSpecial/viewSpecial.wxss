    height: 35rpx;
}

.qrcode-down .title {
    width: 30%;
    margin: 18rpx auto;
    text-align: center;
    font-size: 24rpx;
    color: #b6b6b6;
}

.qrcode-down .barcode {
    width: 100%;
    /* margin: 80rpx auto 0; */
    overflow: hidden;
    font-size: 30rpx;
    color: #333;
    position: absolute;
    text-align: center;
}

.qrcode-down .barcode .image {
    width: 100%;
    height: 120rpx;
    margin-bottom: 18rpx;
}

.qrcode-down .code {
    width: 78%;
    height: 66%;
    margin: 160rpx auto 0;
    text-align: center;
}

.qrcode-down .code .image {
    width: 90%;
    height: 90%;
}

.qrcode-down .tips {
    font-size: 28rpx;
    color: #c9c9c9;
    margin: -86rpx auto 0;
    text-align: center;
}

.animated {
    animation-duration: 0.2s;
    animation-fill-mode: both;
    animation-timing-function: ease-in;
}

.slideInUp {
    animation-name: slideInUp;
}

.slideOutUp {
    animation-name: slideOutUp;
}

.slideInDown {
    animation-name: slideInDown;
}

.slideOutDown {
    animation-name: slideOutDown;
}

@keyframes slideInUp {
    0% {
        transform: translate3d(0, 100%, 0);
        visibility: visible;
    }
    to {
        transform: translateZ(0);
    }
}

@keyframes slideInDown {
    0% {
        transform: translate3d(0, -100%, 0);
        visibility: visible;
    }
    to {
        transform: translateZ(0);
    }
}

@keyframes slideOutUp {
    0% {
        transform: translateZ(0);
    }
    to {
        visibility: hidden;
        transform: translate3d(0, -100%, 0);
    }
}

@keyframes slideOutDown {
    0% {
        transform: translateZ(0);
    }
    to {
        visibility: hidden;
        transform: translate3d(0, 100%, 0);
    }
}

.swiper-item .warp {
    height: 100%;
    overflow: auto;
}


/* ms-Modal */

.ms-Modal,
.ms-Modal::before {
    position: fixed;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 100;
}

.ms-Modal {
    transition: all .3s ease;
    opacity: 0;
}

.ms-Modal::before {
    display: block;
    content: '';
    z-index: 1;
    opacity: .6;
    background-color: #000;
}

.ms-Modal .ms-ModalBody {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    height: 100%;
    width: 100%;
    position: relative;
    z-index: 100;
}

.ms-Modal.cur {
    opacity: 1;
}

.ms-Modal .ms-ModalBody .content {
    border-radius: 8rpx;
    width: 80%;
    display: block;
    background-color: #FFF;
}

.ms-Modal .ms-ModalBody .content .info {
    box-sizing: border-box;
    padding: 30rpx;
}

.ms-Modal .ms-ModalBody .content .hd {
    font-size: 32rpx;
    display: block;
    text-align: center;
}

.ms-Modal .ms-ModalBody .content .txt {
    margin: 30rpx 0;
    display: block;
    font-size: 28rpx;
}

.ms-Modal .ms-ModalBody .content .input {
    box-sizing: border-box;
    padding: 10rpx 20rpx;
    height: 80rpx;
    border-radius: 6rpx;
    border: 1px solid #efefef;
    font-size: 28rpx;
}

.ms-Modal .ms-ModalBody .button {
    border-top: 1px solid #efefef;
    display: flex;
    white-space: nowrap;
    flex-direction: row;
}

.ms-Modal .ms-ModalBody .button .label {
    display: inline-flex;
    flex: 1;
    align-items: center;
    justify-content: center;
    font-size: 28rpx;
    height: 100rpx;
}

.ms-Modal .ms-ModalBody .button .label:first-child {
    border-right: 1px solid #efefef;
}

.ms-Modal .ms-ModalBody .button .label.red {
    color: #E72722;
}