    top: 0;
    transform: translateY(-110%);
    z-index: 1400;
}

.qrcode-down {
    width: 100%;
    height: 82%;
    position: absolute;
    /* border-radius: 16rpx 16rpx 0 0; */
    transform: translateY(110%);
    z-index: 1400;
    top: 18%;
    background: rgba(0, 0, 0, 0.7);
}

.lqrcode_box {
    width: 88%;
    height: 84%;
    position: absolute;
    background: #fff;
    left: 0;
    right: 0;
    margin: 0 auto;
    border-radius: 20rpx;
}

.qrcode-down .icon {
    width: 130rpx;
    height: 130rpx;
    margin: -65rpx auto 0;
    overflow: hidden;
    border-radius: 50%;
    background: #fff;
}

.qrcode-down .icon .image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    display: inline-block;
    margin: 15rpx;
}

.qrcode-down .close {
    width: 70rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    overflow: hidden;
    margin: -40rpx 10rpx;
}

.qrcode-down .close .image {
