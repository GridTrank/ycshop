}

.indexCustomList .scroll-veiwItem {
  position: relative;
  width: calc(100% - 20rpx);
  overflow: hidden;
  margin: 10rpx;
  box-sizing: border-box;
  padding: 0 10rpx 26rpx;
  background: #fff;
}

.indexCustomList .ms-hot {
  padding: 10rpx;
}

.indexCustomList .ms-hot .view-swiper {
