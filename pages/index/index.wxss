/* pages/default/index.wxss */

page {
  height: 100%;
}

.index-banner-swiper {
  overflow: hidden;
}

/* 新人优惠劵弹窗 */

.ms-container-index {
  background: #f5f5f5;
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;

}

.ms-newPopup {
  position: fixed;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 100;
}

.ms-newPopup .content {
  position: relative;
  z-index: 889;
  width: 70%;
}

.ms-newPopup .content .getNewCoupon {
  position: absolute;
  z-index: 10;
  top: 0;
  width: 100%;
  height: 100%;
  left: 0;
  opacity: 0;
}

.ms-newPopup .content image {
  height: auto;
  width: 100%;
}

.ms-newPopup .content .close {
  position: absolute;
  width: 60rpx;
  top: -100rpx;
  right: 0rpx;
}

.ms-newPopup .content .close::after {
  content: "";
  position: absolute;
  height: 44rpx;
  width: 1px;
  background: #f5f5f5;
  top: 60rpx;
  z-index: -1;
  right: 30rpx;
}

.ms-newPopup .content .pic {
  width: 100%;
}

.ms-indexDataContent {
  height: 100%;
  overflow: hidden;
}

.ms-swiperNav {
  display: flex;
  flex-direction: row;
  padding-bottom: 10rpx;
}

.ms-swiperNav .item {
  color: #666;
  box-sizing: border-box;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  height: 50rpx;
  flex: 1;
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
}

.ms-swiperNav .item image {
  margin-right: 10rpx;
  height: 30rpx;
  width: 30rpx;
}

.banner-img {
  width: 100%;
  display: block;
}

.ms-indexFloor {
  background: #fff;
  margin: 0rpx 20rpx 20rpx;
  border-radius: 10rpx;
  overflow: hidden;
}


/**楼层标题1**/

.ms-boxHd {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 30rpx 20rpx 10rpx;
  background: #fff;
}

.ms-boxHd .dec {
  color: #7F7F7F;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}

.ms-boxHd .name {
  display: flex;
  align-items: center;
}

.ms-boxHd .name image {
  width: 116rpx;
}

.ms-boxHd .name .line {
  width: 10rpx;
  height: 30rpx;
  border-radius: 20rpx;
  margin-right: 16rpx;
  background: linear-gradient(to bottom, #EC2A22, #F84D46);
}


/** 新品首发 **/

.scroll-veiwItem {
  width: calc(100% - 40rpx);
  overflow: hidden;
  margin: 0 20rpx;
  display: flex;

}

.no_activity {
  width: 100%;
  height: 100vh;
  position: fixed;
  z-index: 1502;
  display: flex;
  justify-content: center;
  align-items: center;
}

.no_activity .pic {
  background: #fff;
  padding: 80rpx;
  width: 400rpx;
  position: relative;
  z-index: 1503;
  border-radius: 20rpx;
  margin-top: -100rpx;
}

.no_activity .pic image {
  width: 100%;
}

.index_mark {
  width: 100%;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.7);
}

.index_mark image {
  display: inline-block;
  width: 100%;
  margin-top: -80rpx;
}

.index_mark_close {
  border-radius: 50%;
  width: 52rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  border: 1px solid #fff;
  position: absolute;
  bottom: -100rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  color: #fff;
}

.goActive {
  position: fixed;
  width: 88rpx;
  height: 88rpx;
  bottom: 180rpx;
  right: 30rpx;
  z-index: 99;
  border-radius: 50%;
  overflow: hidden;
  box-shadow: 0px 1px 15rpx 0px rgba(0, 0, 0, 0.1);
}

.goTop {
  width: 88rpx;
  height: 88rpx;
  position: fixed;
  justify-content: center;
  align-items: center;
  display: flex;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.9);
  right: 30rpx;
  bottom: 40rpx;
  box-shadow: 0px 1px 15rpx 0px rgba(0, 0, 0, 0.1);
}

.goTop .top {
  width: 37rpx;
}

.goActive image {
  width: 100%;
  height: auto;
}

.bottom-line {
  position: relative;
  font-size: 26rpx;
  padding: 20rpx 0 40rpx;
  text-align: center;
  background: #f5f5f5;
}

.bottom-line text {
  background: #f5f5f5;
  display: inline-block;
  position: relative;
  z-index: 2;
  padding: 0 10rpx;
  color: #999;
  line-height: 40rpx;
}

.bottom-line::after {
  content: "";
  position: absolute;
  width: 220rpx;
  height: 2rpx;
  background: #999;
  left: 50%;
  margin-left: -110rpx;
  top: 39rpx;
}

.ms-indexFloor .ms-boxHd.flex-row {
  justify-content: space-between;
  padding: 30rpx 20rpx 10rpx;
  box-sizing: border-box;
}

.ms-indexFloor .ms-boxHd.line .ms-boxHd {
  border-bottom: 1px solid #eee;
  padding: 30rpx 0rpx 30rpx;
}

.ms-indexFloor .ms-boxHd.line {
  padding: 0 20rpx;
}

.ms-indexFloor .ms-boxHd.none {
  padding: 0;
}

.ms-hot {
  padding-bottom: 10rpx;
}

.ms-hot .view-swiper {
  margin-top: 20rpx;
  height: 396rpx;
  width: 100%;
  padding: 0 10rpx;
  box-sizing: border-box;
}

.ms-hot .view-swiper .wx-swiper-dot {
  width: 12rpx;
  display: inline-flex;
  height: 6rpx;
  justify-content: space-between;
  margin-right: 6rpx;
}

.ms-hot .view-swiper .wx-swiper-dot-active {
  width: 24rpx;
}

.ms-hot .view-swiper .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(238, 238, 238, 1);
  border-radius: 40rpx;
}

.ms-hot .view-swiper .wx-swiper-dot-active::before {
  background: rgba(217, 35, 46, 1);
  border-radius: 40rpx;
  width: 24rpx;
}

.ms-hot .view-swiper .swiper-item {
  box-sizing: border-box;
}

.ms-hot .view-swiper .ms-viewBox {
  padding: 0 10rpx;
}

.ms-hot .view-swiper .ms-viewItem .pic {
  border-radius: 8rpx;
}

.custom-swiper-dot .ms-brand .wx-swiper-dot {
  width: 40rpx;
  display: inline-flex;
  height: 6rpx;
  margin: 0;
  justify-content: space-between;
}

.custom-swiper-dot .ms-brand .wx-swiper-dots {
  background: rgba(238, 238, 238, 1);
  border-radius: 40rpx;
}

.custom-swiper-dot .ms-brand .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(238, 238, 238, 1);
  border-radius: 40rpx;
}

.custom-swiper-dot .ms-brand .wx-swiper-dot-active::before {
  background: rgba(217, 35, 46, 1);
  border-radius: 40rpx;
}


/**导航分类**/

.scroll-view_navFilter {
  white-space: nowrap;
  width: 100%;
  margin-bottom: 16rpx;
}

.scroll-view_navFilter .item {
  display: inline-block;
  height: 60rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  color: #999;
  padding: 0 34rpx;
  margin-right: 10rpx;
  position: relative;
}

.scroll-view_navFilter .item:last-child {
  padding-right: 36rpx;
}

.scroll-view_navFilter .item.cur {
  color: #333;
  font-size: 30rpx;
  font-family: PingFangSC-Medium, 'Heiti SC Medium';
}

.scroll-view_navFilter .item.cur::after {
  display: block;
  content: '';
  height: 4rpx;
  width: 42rpx;
  position: absolute;
  bottom: 0;
  border-radius: 50rpx;
  left: 50%;
  margin-left: -21rpx;
  background-color: #E72722;
}

.scroll-view_navFilter .item:last-child {
  margin: 0;
}

.ms-header {
  background-color: #FFF;
  /* position: fixed;
top: 0;
left: 0; */
  box-sizing: border-box;
  width: 100%;
  /* z-index: 10; */
}


/**搜索**/

.ms-gotoSearchPage {
  padding-bottom: 18rpx;
  display: flex;
  width: 100%;
  align-items: center;
}

.ms-gotoSearchPage view {
  background-color: #f3f4f6;
  display: flex;
  height: 70rpx;
  flex: 2;
  border-radius: 100rpx;
  justify-content: center;
  align-items: center;
}

.ms-gotoSearchPage .logo {
  width: 66rpx;
  height: 66rpx;
  flex-shrink: 0;
  margin: 0 20rpx;
}

.ms-gotoSearchPage view label {
  color: #999;
  font-size: 26rpx;
}

.ms-gotoSearchPage view image {
  margin-right: 10rpx;
  height: 30rpx;
  width: 30rpx;
}

.product-warp .like-title {
  padding-bottom: 20rpx;
  text-align: center;
}

.product-warp .like-title .center {
  font-size: 32rpx;
  height: 50rpx;
  position: relative;
}

.product-warp .like-title .center::before {
  content: "";
  position: absolute;
  width: 120rpx;
  height: 4rpx;
  border-radius: 10rpx;
  left: -140rpx;
  top: 50%;
  margin-top: -2rpx;
  background: linear-gradient(to left, #E72722, #f5f5f5);
}

.product-warp .like-title .center::after {
  content: "";
  position: absolute;
  width: 120rpx;
  border-radius: 10rpx;
  height: 4rpx;
  right: -140rpx;
  top: 50%;
  margin-top: -2rpx;
  background: linear-gradient(to right, #E72722, #f5f5f5);
}

.product-warp .list {
  display: flex;
  padding: 0 20rpx 20rpx;
  background: #f5f5f5;
  justify-content: space-between;
}

.product-warp .ms-viewBox {
  background: #f5f5f5;
  flex-direction: column;
  flex-wrap: nowrap;
  padding: 0;
}

.product-warp .ms-viewItem .info .priceWarp .add-cart .add {
  width: 42rpx;
  height: 42rpx;
}

.product-warp .ms-viewBox .ms-viewItem .info .title {
  padding: 0 20rpx;
}

.product-warp .ms-viewBox .ms-viewItem .info .dec {
  padding: 0 20rpx;
}

.product-warp .ms-viewBox .ms-viewItem .info .priceWarp {
  padding: 0 20rpx;
}

.product-warp .ms-viewBox .ms-viewItem .info .tag {
  padding: 0 20rpx;
}

.indexRow .ms-viewBox .ms-viewItem {
  width: 690rpx;
  margin-bottom: 10rpx;
  border-radius: 10rpx;
}

.indexRow .ms-viewBox {
  padding: 10rpx 10rpx 0;

}

.indexListProduct .ms-viewBox {
  background: #f5f5f5;
}

.indexListProduct .ms-viewBox .ms-viewItem {

  border-radius: 8rpx;
  padding: 0;
}

.indexListProduct .activityList .ms-viewItem {
  width: 340rpx;
}

.indexListProduct .ms-viewBox .ms-viewItem .info {
  height: 100%;

}

.indexListProduct .ms-viewBox .ms-viewItem .info .tag,
.indexListProduct .ms-viewBox .ms-viewItem .info .name-info,
.indexListProduct .ms-viewBox .ms-viewItem .info .priceWarp {
  padding: 0 20rpx;
}

.indexListProduct .ms-viewBox .ms-viewItem .info .priceWarp .add-cart .add {
  width: 42rpx;
