  font-size: 28rpx;
  text-align: center;
  height: 80rpx;
  display: inline-block;
}

.sm-title::after,
.sm-title::before {
  position: absolute;
  top: 40rpx;
  content: "";
  width: 100rpx;
  height: 1px;
  background: #cea47d;
}

.sm-title::after {
  left: -120rpx;
}

.sm-title::before {
  right: -120rpx;
}

.indexCustomList {
