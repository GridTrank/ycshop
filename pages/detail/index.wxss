/* swiper */

.swiper-warp {
    position: relative;
    border-bottom: 1px solid #e6e6e6;
}

.swiper-warp .share {
    position: absolute;
    right: 40rpx;
    top: 150rpx;
    z-index: 10;
}

.swiper-warp .share image {
    width: 80rpx;
    height: 80rpx;
}

.presell-info {
    background: #d0232e;
    display: flex;
    justify-content: center;
    height: 120rpx;
    width: 100%;
    z-index: 10;
}

.presell-info .info {
    flex: 2;
    background: #e12a35;
    padding: 10rpx 40rpx 14rpx 16rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.presell-info .info .h2 {
    font-size: 28rpx;
    color: #fff;
    font-weight: 600;
    font-family: PingFangSC, helvetica, 'Heiti SC';
}

.presell-info .info .p {
    font-size: 22rpx;
    color: #fff;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.presell-info .downTime {
    position: relative;
    background: #fea659;
    padding-right: 20rpx;
    font-size: 22rpx;
    min-width: 200rpx;
    display: flex;
    flex-direction: column;
    align-content: center;
    justify-content: center;
}

.presell-info .downTime .day {
    color: #fff;
    font-size: 24rpx;
    margin-right: 4rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.presell-info .downTime .day text {
    color: #fff;
}

.presell-info .downTime.end {
    background: #ccc;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
}

.presell-info .downTime.end::after {
    border-right: 30rpx solid #ccc;
}

.presell-info .downTime::after {
    content: "";
    width: 0;
    height: 0;
    position: absolute;
    left: -28rpx;
    top: 0;
    z-index: 10;
    border-top: 60rpx solid transparent;
    border-bottom: 60rpx solid transparent;
    border-right: 30rpx solid #fea659;
}

.presell-info .time {
    display: flex;
    justify-content: center;
    align-items: center;
    font-weight: 600;
    margin-top: 6rpx;
}

.presell-info .time .item {
    background: #5a3615;
    color: #fff;
    margin: 0 4rpx;
    font-weight: 500;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 36rpx;
    height: 36rpx;
    border-radius: 6rpx;
    flex-shrink: 0;
}

.presell-info .text {
    color: #fff;
    text-align: center;
    line-height: 40rpx;
}

.specail-info .info {
    flex: 1;
    background: #d0232e;
}

.specail-info .timeInfo {
    font-size: 24rpx;
    flex: 1;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: flex-end;
