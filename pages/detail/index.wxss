.ms-productDetail .ms-jsChooseSpec {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    position: relative;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    height: 80rpx;
    border-bottom: 1px solid #efefef;
    padding-right: 20rpx;
}

.ms-productDetail .ms-jsChooseSpec>view {
    font-size: 28rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    color: #666;
}

.ms-productDetail .ms-jsChooseSpec .spec_info {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    max-width: 100%;
    font-size: 26rpx;
    font-weight: 400;
    margin-right: 2rpx;
}

.ms-productDetail .ms-jsChooseSpec .spec_info text:last-child {
    display: none;
}

.ms-productDetail .ms-jsChooseService {
    display: flex;
    justify-content: space-around;
    align-items: center;
    box-sizing: border-box;
    padding: 30rpx 30rpx 20rpx 0;
    font-size: 28rpx;
}

.ms-productDetail .ms-jsChooseService .l {
    align-self: flex-start;
    font-size: 28rpx;
    padding-left: 10rpx;
    color: #666;
    flex-shrink: 0;
    width: 130rpx;
}

.ms-productDetail .ms-jsChooseService .m {
    flex: 1;
}
.ms-productDetail .ms-jsChooseService .m>view{
    display: flex;
}
.ms-productDetail .ms-jsChooseService text {
   
    box-sizing: border-box;
    padding: 0 12rpx 0 30rpx;
    position: relative;
    width: 270rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 24rpx;
}

.ms-productDetail .ms-jsChooseService text:after {
    content: '';
    display: block;
    width: 10px;
    height: 10px;
    border: 2rpx solid #E72722;
    position: absolute;
    top: 50%;
    box-sizing: border-box;
    margin-top: -5px;
    left: 0rpx;
    border-radius: 50%;
}

.ms-productDetail .ms-jsChooseService text:before {
    content: '';
    display: block;
    width: 4px;
    height: 4px;
    background-color: #E72722;
    border-radius: 50%;
    position: absolute;
    top: 50%;
    margin-top: -2px;
    left: 3px;
}

.ms-productDetail .ms-jsChooseService .icon {
    display: flex;
    align-items: center;
}

