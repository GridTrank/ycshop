/* 优惠券选择弹窗 */

.promotionPopup {
    position: fixed;
    background-color: #fff;
    z-index: 1200;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 700rpx;
    font-size: 28rpx;
    transition: transform 0.2s cubic-bezier(0, 0, 0.25, 1);
    transform: translate3d(0, 100%, 0);
}

.promotionPopup .h4 {
    padding: 20rpx 20rpx 0rpx 20rpx;
    font-size: 28rpx;
}

.promotionPopup.show {
    transform: translate3d(0, 0, 0);
}

.promotionPopup.share {
    height: 380rpx;
}

.promotionPopup .title {
    position: relative;
    height: 100rpx;
    line-height: 100rpx;
    background: #fff;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    color: #333333;
    box-sizing: border-box;
    border-bottom: 2rpx solid #ededed;
}

.promotionPopup .title image {
    width: 30rpx;
}

.promotionPopup .promotion-list {
    padding-left: 20rpx;
    height: 620rpx;
    overflow-y: scroll;
}

.promotionPopup .promotion-item {
    padding: 20rpx 60rpx 20rpx 0;
    line-height: 40rpx;
    border-bottom: 0.8px solid #ededed;
    position: relative;
}

.promotionPopup .promotion-item image {
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -10rpx;
}

.promotionPopup .promotion-item .ms-tag {
    background: #fda253;
    color: #fff;
    font-size: 20rpx;
    padding: 4rpx 10rpx;
    margin-right: 10rpx;
    border-radius: 8rpx;
}

.promotionPopup .share-warp {
    display: flex;
    justify-content: space-around;
    align-items: center;
    height: 300rpx;
}

.promotionPopup .share-warp .item {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column;
    color: #666;
    font-size: 26rpx;
    position: relative;
}
.promotionPopup .share-warp .item .load{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 1;
}
.promotionPopup .share-warp .item>view {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column;
}

.promotionPopup .share-warp .item button {
    background: transparent;
    border: none;
    width: 96rpx;
    padding: 0;
    height: 116rpx;
}

.promotionPopup .share-warp .item button.getUserInfo {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
}

.promotionPopup .share-warp .item button::after {
    border: none;
}

.promotionPopup .share-warp image {
    width: 96rpx;
    margin-bottom: 20rpx;
}

