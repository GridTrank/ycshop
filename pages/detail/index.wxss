page {
    height: 100%;
    background: #f5f5f5;
}


/* swiper */

.swiper-warp {
    position: relative;
    border-bottom: 1px solid #e6e6e6;
}

.swiper-warp .share {
    position: absolute;
    right: 40rpx;
    top: 150rpx;
    z-index: 10;
}

.swiper-warp .share image {
    width: 80rpx;
    height: 80rpx;
}

.presell-info {
    background: #d0232e;
    display: flex;
    justify-content: center;
    height: 120rpx;
    width: 100%;
    z-index: 10;
}

.presell-info .info {
    flex: 2;
    background: #e12a35;
    padding: 10rpx 40rpx 14rpx 16rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.presell-info .info .h2 {
    font-size: 28rpx;
    color: #fff;
    font-weight: 600;
    font-family: PingFangSC, helvetica, 'Heiti SC';
}

.presell-info .info .p {
    font-size: 22rpx;
    color: #fff;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.presell-info .downTime {
    position: relative;
    background: #fea659;
    padding-right: 20rpx;
    font-size: 22rpx;
    min-width: 200rpx;
    display: flex;
    flex-direction: column;
    align-content: center;
    justify-content: center;
}

.presell-info .downTime .day {
    color: #fff;
    font-size: 24rpx;
    margin-right: 4rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.presell-info .downTime .day text {
    color: #fff;
}

.presell-info .downTime.end {
    background: #ccc;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
}

.presell-info .downTime.end::after {
    border-right: 30rpx solid #ccc;
}

.presell-info .downTime::after {
    content: "";
    width: 0;
    height: 0;
    position: absolute;
    left: -28rpx;
    top: 0;
    z-index: 10;
    border-top: 60rpx solid transparent;
    border-bottom: 60rpx solid transparent;
    border-right: 30rpx solid #fea659;
}

.presell-info .time {
    display: flex;
    justify-content: center;
    align-items: center;
    font-weight: 600;
    margin-top: 6rpx;
}

.presell-info .time .item {
    background: #5a3615;
    color: #fff;
    margin: 0 4rpx;
    font-weight: 500;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 36rpx;
    height: 36rpx;
    border-radius: 6rpx;
    flex-shrink: 0;
}

.presell-info .text {
    color: #fff;
    text-align: center;
    line-height: 40rpx;
}

.specail-info .info {
    flex: 1;
    background: #d0232e;
}

.specail-info .timeInfo {
    font-size: 24rpx;
    flex: 1;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: flex-end;
    flex-direction: column;
}

.specail-info .store {
    display: flex;
    justify-content: center;
    align-items: center;
}

.specail-info .store .passage {
    background: #b22433;
    height: 10rpx;
    border-radius: 50rpx;
    width: 100rpx;
    margin-right: 10rpx;
    position: relative;
    margin-top: 4rpx;
}

.specail-info .time-warp {
    display: flex;
    justify-content: center;
    align-items: center;
}

.specail-info .time-warp image {
    width: 30rpx;
    height: 30rpx;
    margin: 0 30rpx 0 20rpx;
}

.specail-info .store .passage>view {
    position: absolute;
    left: 0;
    top: 0;
    height: 10rpx;
    background: #fff;
    border-radius: 50rpx;
}

.specail-info view {
    color: #fff;
}

.specail-info .time {
    font-weight: 500;
}

.swiper-warp .swiper-dot {
    position: absolute;
    font-size: 30rpx;
    color: #999;
    right: 26rpx;
    bottom: 30rpx;
    z-index: 2;
}

.cpns_tag {
    width: 100%;
    z-index: 10;
}

.cpns_tag image {
    width: 100%;
    display: block;
}


/* 优惠券选择弹窗 */

.promotionPopup {
    position: fixed;
    background-color: #fff;
    z-index: 1200;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 700rpx;
    font-size: 28rpx;
    transition: transform 0.2s cubic-bezier(0, 0, 0.25, 1);
    transform: translate3d(0, 100%, 0);
}

.promotionPopup .h4 {
    padding: 20rpx 20rpx 0rpx 20rpx;
    font-size: 28rpx;
}

.promotionPopup.show {
    transform: translate3d(0, 0, 0);
}

.promotionPopup.share {
    height: 380rpx;
}

.promotionPopup .title {
    position: relative;
    height: 100rpx;
    line-height: 100rpx;
    background: #fff;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    color: #333333;
    box-sizing: border-box;
    border-bottom: 2rpx solid #ededed;
}

.promotionPopup .title image {
    width: 30rpx;
}

.promotionPopup .promotion-list {
    padding-left: 20rpx;
    height: 620rpx;
    overflow-y: scroll;
}

.promotionPopup .promotion-item {
    padding: 20rpx 60rpx 20rpx 0;
    line-height: 40rpx;
    border-bottom: 0.8px solid #ededed;
    position: relative;
}

.promotionPopup .promotion-item image {
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -10rpx;
}

.promotionPopup .promotion-item .ms-tag {
    background: #fda253;
    color: #fff;
    font-size: 20rpx;
    padding: 4rpx 10rpx;
    margin-right: 10rpx;
    border-radius: 8rpx;
}

.promotionPopup .share-warp {
    display: flex;
    justify-content: space-around;
    align-items: center;
    height: 300rpx;
}

.promotionPopup .share-warp .item {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column;
    color: #666;
    font-size: 26rpx;
    position: relative;
}
.promotionPopup .share-warp .item .load{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 1;
}
.promotionPopup .share-warp .item>view {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column;
}

.promotionPopup .share-warp .item button {
    background: transparent;
    border: none;
    width: 96rpx;
    padding: 0;
    height: 116rpx;
}

.promotionPopup .share-warp .item button.getUserInfo {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
}

.promotionPopup .share-warp .item button::after {
    border: none;
}

.promotionPopup .share-warp image {
    width: 96rpx;
    margin-bottom: 20rpx;
}

.overflow {
    height: 100%;
    overflow: hidden;
}

.ms-indexFloor {
    background: #fff;
    margin-bottom: 20rpx;
}

.ms-productDetail {
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding:30rpx;
}

.ms-productDetail .selectedSpec {
    max-width: 60%;
    text-align: right;
    position: relative;
}

.ms-productDetail .chooseSpecWarp {
    position: relative;
    padding-left: 10rpx;
}

.ms-productDetail .chooseSpecWarp .btn {
    position: absolute;
    width: 100%;
    height: 100%;
    opacity: 0;
    left: 0;
    z-index: 100;
    top: 0;
}

.chooseNum {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 14rpx 30rpx 14rpx 0;
    font-size: 28rpx;
    border-bottom: 1px solid #efefef;
}

.chooseNum .title {
    font-size: 28rpx;
    color: #666;
    padding-left: 10rpx;
}

.chooseNum .select_num button.minus {
    border-radius: 100rpx 0 0 100rpx;
}

.chooseNum .select_num button {
    display: inline-block;
    vertical-align: middle;
    height: 56rpx;
    width: 72rpx;
    line-height: 48rpx;
    box-sizing: border-box;
    background: #fff;
    border-radius: 0 100rpx 100rpx 0;
    border: 1px solid #efefef;
    padding: 0;
}

.chooseNum .select_num button::after {
    border: none;
    border-radius: 0rpx;
}

.chooseNum .select_num input {
    border-top: 1px solid #efefef;
    border-bottom: 1px solid #efefef;
    display: inline-block;
    vertical-align: middle;
    height: 52rpx;
    width: 72rpx;
    font-size: 28rpx;
    text-align: center;
}

.getPhoneNumber {
    position: absolute;
    opacity: 0;
    z-index: 10;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}



