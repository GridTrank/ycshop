    width: 100rpx;
    margin-right: 10rpx;
    position: relative;
    margin-top: 4rpx;
}

.specail-info .time-warp {
    display: flex;
    justify-content: center;
    align-items: center;
}

.specail-info .time-warp image {
    width: 30rpx;
    height: 30rpx;
