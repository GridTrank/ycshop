page{
  background: #f4f4f4;
}
.person-info {
  margin-top: 10rpx;
}
.brand-info {
  margin-top: 20rpx;
}
.ms-container-account .title-warp {
  border-bottom: 1px solid #DCDCDC;
  background: #fff;
  padding: 30rpx 30rpx 20rpx;
  min-height: 98rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.ms-container-account .title-warp .title {
  display: flex;
  font-size: 32rpx;
  justify-content: space-between;
  align-items: center;
}
.ms-container-account .title-warp .dec {
  font-size: 22rpx;
  color: #999;
  margin-top: 10rpx;
}
.ms-container-account .title-warp .title .icon {
  width: 40rpx;
  margin-right: 12rpx;
}

.ms-container-account .title-warp .title .name {
  display: flex;
  align-items: center;
  font-weight: 600;
}

.ms-container-account .title-warp .title .info {
  display: flex;
  align-items: center;
 
}
.ms-container-account .title-warp .title .info text {
  font-size: 26rpx;
  color: #169BD5;
  margin-right: 10rpx;
}
.ms-container-account .title-warp .title .red {
  font-size: 32rpx;
  color: #e23232;
  font-weight: 600;
  margin-left: 10rpx;
}


.form-warp {
  padding: 0 30rpx;
  background: #fff;
}
.form-warp .checkbox {
  display: flex;
  font-size: 24rpx;
  color: #666;
  padding: 20rpx 0;
}

.form-warp .checkbox .agreement navigator{
  color: #e23232;
  display: inline-block;
}
.form-warp .form-cell {
  height: 96rpx;
  border-bottom: 1px solid #DCDCDC;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.form-warp .form-cell:last-child{
  border-color: #fff;
}
.form-warp .form-cell .name-warp{
  font-size: 28rpx;
  display: flex;
  flex: 2;
  align-items: center;
}
.form-warp .form-cell .name {
  width: 130rpx;
}

.form-warp .form-cell .error {
  display: none;
  font-size: 24rpx;
  color: #e23232;
}
.form-warp .form-cell.error .error{
  display: block;
}

.form-warp .form-cell.error {
  border-color: #e23232;
}
.form-warp .form-cell .con {
  font-size: 26rpx;
  color: #999;
  flex: 3;
  height: 100%;
  align-items: center;
  display: flex;
  justify-content: flex-end;
}
.form-warp .form-cell .con .photo{
  width: 80rpx;
  border-radius: 50%;
}
.form-warp .form-cell input {
  color: #666;
  height: 100%;
  text-align: right;
  padding-right: 1rpx;
  width: 380rpx;
}

.btn-warp {
  padding: 60rpx 24rpx;
}
.btn-warp button {
  width: 702rpx!important;
  box-sizing: border-box;
  height: 80rpx;
  font-weight: 400;
  line-height: 80rpx;
  background: #e23232;
  color: #fff;
  font-size: 32rpx;
  border-radius: 100rpx;
  padding: 0;
}

.btn-warp button[disabled]{
  background-color: #e23232!important;
  opacity: 0.7;
  color: #fff!important;
}
.form-warp .form-cell input[disabled]{
  color: #999;
}
.form-warp .form-cell input[disabled] .place-holder{
  color: #999;
}
.ms-container-account .form-warp .form-cell.error .bank_name{
  color: #e23232!important;
}