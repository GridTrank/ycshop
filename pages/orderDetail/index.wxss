page {
    background: #f4f4f4 !important;
}


/**数据 item 遍历**/

.ms-orderItem {
    margin-top: 20rpx;
}

.orderCell {
    display: flex;
    width: 100%;
    font-size: 28rpx;
    box-sizing: border-box;
    padding: 20rpx 30rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1px solid #efefef;
    background: #fff;
}

.orderCell>view {
    display: flex;
    justify-content: center;
    align-items: center;
}
.orderCell .photo{
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    margin-right: 10rpx;
  }
.orderCell .tag {
    background: #E72722;
    padding: 2rpx 14rpx;
    color: #fff;
    font-size: 20rpx;
    border-radius: 4rpx;
    margin-left: 14rpx;
}

.orderCell .type.red {
    color: #E72722;
}

.orderCell .status {
    display: flex;
}

.orderCell .status .ms-icon {
    position: relative;
    margin-left: 20rpx;
    overflow: visible;
}

.orderCell .status .ms-icon::after {
    position: absolute;
    content: "";
    width: 2rpx;
    left: -16rpx;
    height: 30rpx;
    top: 6rpx;
    background: #d6d6d6;
}

.ms-orderItem .orderCell .ms-icon {
    width: 38rpx;
    height: 38rpx;
}

.orderCell .button {
    color: #353535;
    width: 162rpx;
    height: 64rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 24rpx;
    border-radius: 50rpx;
    border: 1px solid #cbcbcb;
}

.ms-orderItem .orderMoney {
    background: #fff;
    padding: 20rpx;
}

.ms-orderItem .orderMoney .btn-warp {
    float: right;
    height: 64rpx;
}

.ms-orderItem .shopInfo .image {
    display: block;
    width: 160rpx;
    margin-right: 20rpx;
    height: 160rpx;
    background: #F9FAFC;
}

.ms-orderItem .shopInfo .image.ellipsis {
    background: #fff;
    width: 50rpx;
    height: 50rpx;
    margin-left: 40rpx;
}

.ms-orderItem .orderShop {
    display: flex;
    justify-content: space-between;
    background: #fff;
}

.ms-orderItem .money {
    border-bottom: 1px solid #efefef;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
}

.ms-orderItem .money .text {
    font-size: 22rpx;
    color: #666;
}

.ms-orderItem .money .text.symbol {
    color: #333;
    margin-right: 10rpx;
    display: inline-block;
}

.ms-orderItem .money .text.gray {
    color: #666;
}

.orderShop .shopDetail {
    display: flex;
}

.ms-orderItem .orderType {
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    font-size: 24rpx;
    color: #E72722;
}

.ms-orderItem .orderType.gray {
    color: #333;
}

.orderShop .shopDetail .pic {
    margin: 20rpx 32rpx 20rpx 30rpx;
}

.orderShop .shopDetail .info {
    padding-top: 20rpx;
    font-size: 24rpx;
    width: 460rpx;
    line-height: 40rpx;
}

.orderShop .shopDetail .dec {
    font-size: 24rpx;
    color: #666;
}
.orderShop .shopDetail .tag {
    display: inline-block;
    padding: 0 10rpx;
    color: #d9232e;
    border:1px solid #d9232e;
    font-size: 20rpx;
    border-radius: 10rpx;
    line-height: 26rpx;
  }
.orderShop .shopImg {
    padding: 20rpx;
    display: flex;
    padding-right: 0;
    overflow: hidden;
    justify-content: center;
    align-items: center;
}

.ms-container-orderDetail {
    padding-bottom: 100rpx;
}

.ms-container-orderDetail .shopDetail image {
    width: 160rpx;
    height: 160rpx;
    display: block;
}

.ms-container-orderDetail .shopDetail .info {
    line-height: 44rpx;
    flex: 2;
    /* width: calc(100% - 200rpx); */
}

.ms-container-orderDetail .presell-tip {
    background: #faeedc;
    color: #fda253;
    font-size: 24rpx;
    padding: 16rpx 20rpx;
}

.ms-container-orderDetail .shopDetail .info .title text {
    color: #E72722;
}

.ms-container-orderDetail .shopDetail .info .title .tag {
    background: #E72722;
    color: #fff;
    padding: 2rpx 14rpx;
    margin-right: 14rpx;
    font-size: 22rpx;
    border-radius: 4rpx;
}

.ms-container-orderDetail .shopDetail .info .title.indent {
    text-indent: -14rpx;
}

.ms-container-orderDetail .shopDetail .info .price .symbol {
    font-size: 20rpx;
}

.ms-container-orderDetail .shopDetail {
    border-bottom: 1px solid #efefef;
}

.ms-container-orderDetail .shopList {
    background: #fff;
}

.ms-container-orderDetail .shopNum {
    font-size: 24rpx;
    margin: 20rpx 30rpx;
}

.ms-container-orderDetail .orderBtn {
    text-align: right;
    position: fixed;
    bottom: 0;
    width: 100%;
    border-top: 1px solid #efefef;
    display: block;
}

.ms-container-orderDetail .orderBtn.none {
    position: static;
    display: flex;
}

.ms-container-orderDetail .orderInfo {
    border-top: 1px solid #efefef;
}

.ms-container-orderDetail .orderInfo>view {
    position: relative;
}

.ms-container-orderDetail .orderInfo .ms-icon {
    width: 38rpx;
    height: 38rpx;
}

.ms-container-orderDetail .orderMoney {
    background: #fff;
    font-size: 28rpx;
    padding-left: 30rpx;
    line-height: 44rpx;
    margin: 0rpx 0 20rpx;
}

.ms-container-orderDetail .addressInfo {
    padding: 20rpx 30rpx 20rpx;
    border-bottom: 1px solid #efefef;
    background: #fff;
    font-size: 28rpx;
    line-height: 44rpx;
    margin: 0 0 20rpx 0;
}

.ms-container-orderDetail .addressInfo .name text {
    vertical-align: middle;
}

.ms-container-orderDetail .addressInfo .name text.na {
    width: 150rpx;
    margin-right: 10rpx;
    display: inline-block;
    vertical-align: middle;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.ms-container-orderDetail .addressInfo .address {
    color: #7f7f7f;
    text-align: left;
    line-height: 36rpx;
}

.ms-container-orderDetail .moneyInfo {
    padding: 20rpx 20rpx 20rpx 0;
}

.ms-container-orderDetail .moneyInfo>view {
    display: flex;
}

.ms-container-orderDetail .moneyInfo .name {
    display: inline-block;
    width: 190rpx;
    color: #7f7f7f;
}

.ms-container-orderDetail .moneyInfo .name text {
    width: 56rpx;
    display: inline-block;
}

.ms-container-orderDetail .moneyInfo text.symbol {
    font-size: 20rpx;
    width: auto;
}

.ms-container-orderDetail .moneyInfo .red {
    color: #E72722;
}

.ms-container-orderDetail .moneyInfo .money .symbol {
    color: #E72722;
}

.ms-container-orderDetail .serviceWarp {
    background: #fff;
    font-size: 28rpx;
    padding: 20rpx;
}

.ms-container-orderDetail .serviceTime {
    color: #7f7f7f;
    margin-bottom: 30rpx;
}

.ms-container-orderDetail .btnWarp {
    display: flex;
    justify-content: center;
    margin-bottom: 40rpx;
}

.ms-container-orderDetail .btnWarp .item {
    border: 1px solid #cbcbcb;
    position: relative;
    height: 60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50rpx;
    width: 202rpx;
    margin: 0 20rpx;
}

.ms-container-orderDetail .btnWarp .item contact-button {
    position: absolute;
    width: 100%;
    opacity: 0;
    z-index: 10;
    left: 0;
    top: 0;
    background: transparent;
}

.ms-container-orderDetail .btnWarp .item text {
    left: 0;
    position: absolute;
    width: 100%;
    text-align: center;
}

.copy {
    position: absolute;
    right: 20rpx;
    width: 26rpx;
    top: 10rpx;
}

.callerPopup {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 10;
}

.callerPopup .popup-warp {
    position: absolute;
    top: 20%;
    z-index: 999;
    width: 70%;
    background: #fff;
    left: 15%;
    border-radius: 10rpx;
    text-align: center;
    line-height: 50rpx;
    padding: 40rpx 0 0;
}

.callerPopup .popup-warp .title {
    font-size: 28rpx;
}

.callerPopup .popup-warp .close {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    right: 0;
    top: 0;
    text-align: center;
    line-height: 40rpx;
    display: block;
    color: #999;
}

.callerPopup .popup-warp .contact-button {
    position: relative;
    width: 100%;
    height: 80rpx;
    border-top: 1px solid #efefef;
    margin-top: 40rpx;
}

.callerPopup .popup-warp .contact-button text {
    position: absolute;
    width: 100%;
    line-height: 80rpx;
    left: 0;
    font-size: 30rpx;
    top: 0;
    text-align: center;
}

.callerPopup .popup-warp .item {
    font-size: 50rpx;
    color: #333;
    margin-top: 10rpx;
    font-family: PingFangSC, helvetica, 'Heiti SC';
}

.callerPopup .popup-warp .time {
    color: #999;
    font-size: 26rpx;
}