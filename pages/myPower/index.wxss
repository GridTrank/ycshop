page {
    height: 100%;
}

.ms-container-power {
    height: 100%;
}

.powerScroll {
    height: calc(100% - 96rpx);
    margin-top: 20rpx;
}

.no-record {
    height: 80%;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
}

.no-record .warp {
    width: 100%;
    text-align: center;
    color: #7f7f7f;
}

.no-record image {
    display: block;
    width: 50%;
    margin: 0 auto 20rpx;
}

.ms-powerItem {
    background: #fff;
    margin-bottom: 20rpx;
    display: flex;
}

.ms-powerItem .img {
    width: 220rpx;
    height: 220rpx;
    overflow: hidden;
}

.ms-powerItem .pic {
    width: 220rpx;
    height: 220rpx;
    padding: 30rpx;
    position: relative;
}

.ms-powerItem .pic image {
    width: 100%;
}

.ms-powerItem .pic .state {
    position: absolute;
    left: 16rpx;
    top: 10rpx;
    font-size: 20rpx;
    color: #fff;
    padding-left: 6rpx;
    text-align: center;
    line-height: 46rpx;
    width: 100rpx;
    height: 80rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}

.ms-powerItem .info {
    padding: 30rpx 30rpx 30rpx 0;
    width: calc(100% - 280rpx);
    box-sizing: border-box;
    position: relative;
}

.ms-powerItem .title {
    font-size: 28rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.ms-powerItem .progress {
    width: 100%;
    margin: 20rpx 0 10rpx 0;
    height: 8rpx;
    background: #e5e5e5;
    border-radius: 30rpx;
}

.ms-powerItem .progress .bar {
    background: #E72722;
    height: 8rpx;
    border-radius: 30rpx;
}

.ms-powerItem .power .num {
    font-size: 22rpx;
    color: #666;
}

.ms-powerItem .power .num text {
    color: #E72722;
}

.ms-powerItem .btn {
    position: absolute;
    font-size: 24rpx;
    color: #fff;
    border-radius: 100rpx;
    width: 170rpx;
    height: 60rpx;
    background: #E72722;
    display: flex;
    bottom: 30rpx;
    right: 30rpx;
    justify-content: center;
    align-items: center;
    background: linear-gradient(to right, #db2831, #fe8e6f);
    box-shadow: 0 0 16rpx #fe8e6f;
}

.ms-powerItem.end .pic .state {
    background-image: url(data:image/png;base64,);
}

.ms-powerItem.end .btn {
    background: #c1c1c1;
    box-shadow: none;
}

.ms-powerItem.end .progress .bar {
    background: #c1c1c1;
}

.ms-powerItem.end .power .num {
    color: #999;
}

.ms-powerItem.end .power .num text {
    color: #999;
}