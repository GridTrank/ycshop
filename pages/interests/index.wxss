.cart-warp {
  padding: 40rpx 30rpx;
  background: linear-gradient(to bottom, #414141, #080808);
}

.cart-warp image {
  width: 100%;
}

.ms-boxHd {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 30rpx 20rpx 10rpx;
  background: #fff;
}

.ms-boxHd .name {
  display: flex;
  align-items: center;
}

.ms-boxHd .name text {
  font-weight: 600;
}

.ms-boxHd .name .line {
  width: 10rpx;
  height: 30rpx;
  border-radius: 20rpx;
  margin-right: 16rpx;
  background: linear-gradient(135deg, #EC2A22, #F84D46);
}

.interests-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
}

.interests-list .item {
  width: 30%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 20rpx 0;
}

.interests-list image {
  width: 110rpx;
  height: 110rpx;
}

.interests-list view {
  font-size: 30rpx;
  line-height: 42rpx;
  
}
.interests-list view view{
  margin: 13rpx 0;
}
.interests-list text {
  font-size: 24rpx;
  color: #999;
}

.btn-warp {
  padding: 63rpx 30rpx 26rpx;
  display: flex;
  background: #f5f5f5;
}

.btn-warp navigator{
  background: linear-gradient(90deg, #F54F47,#E72722);
  height: 92rpx;
  color: #fff;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 690rpx!important;
  border-radius: 100rpx;
  font-weight: 400;
}