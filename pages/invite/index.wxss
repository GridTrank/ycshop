.ms-lineHDMid {
    background: #fff;
    font-size: 30rpx;
    padding: 40rpx;
    text-align: center;
}

.swiper-warp {
    position: relative;
    overflow: hidden;
}

.swiper-warp .rule {
    position: absolute;
    width: 160rpx;
    height: 70rpx;
    right: -30rpx;
    z-index: 10;
    top: 50rpx;
    font-size: 30rpx;
    line-height: 70rpx;
    text-align: center;
    background: #fff;
    border-radius: 80rpx;
    padding: 0 30rpx;
}

.ms-viewBox {
    margin-top: 0;
    background: #fff;
}

.activity_info {
    background: #fff;
    padding: 30rpx 0 30rpx 30rpx;
    display: flex;
}

.friendsHelp .noHelp {
    padding: 30rpx 0 0;
    text-align: center;
    font-size: 30rpx;
    color: #999;
}

.activity_info .info {
    width: calc(100% - 270rpx);
}

.activity_info .info .title {
    font-size: 34rpx;
    font-weight: 600;
    line-height: 60rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.activity_info .info .remark {
    font-size: 24rpx;
    color: #7f7f7f;
    padding-right: 20rpx;
}

.ms-container-invite .paly {
    margin-bottom: 14rpx;
}

.ms-container-invite .paly image {
    width: 100%;
    display: block;
}

.activity_info {
    margin-bottom: 20rpx;
}

.activity_info .info .price {
    font-size: 32rpx;
    color: #E72722;
    font-weight: 600;
    margin-top: 30rpx;
    margin-bottom: 10rpx;
}

.activity_info .detail {
    font-size: 24rpx;
    position: relative;
    width: 270rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-left: 1px dashed #efefef;
}

.activity_info .detail navigator {
    border: 1px solid #bfbfbf;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #7f7f7f;
    padding: 15rpx 30rpx;
    border-radius: 40rpx;
}

.countDown {
    background: #fff;
    padding: 40rpx 60rpx;
    overflow: hidden;
}

.countDown .line {
    position: relative;
}

.countDown .line::after {
    content: "";
    position: absolute;
    width: 100%;
    height: 2rpx;
    background: #efefef;
    top: 50%;
}

.countDown .timeDown {
    position: relative;
    z-index: 2;
    margin: 0 auto;
    width: 440rpx;
    text-align: center;
    background: #fff;
    line-height: 50rpx;
    font-size: 30rpx;
}

.countDown .timeDown text {
    font-size: 34rpx;
    font-weight: 600;
    margin: 0 4rpx;
    text-align: center;
    line-break: 60rpx;
    display: inline-block;
}

.countDown .timeDown .time {
    background-color: #333;
    color: #fff;
    font-weight: 400;
    border-radius: 6rpx;
    width: 50rpx;
    height: 50rpx;
}

.progressBar .title {
    line-height: 80rpx;
    font-size: 24rpx;
    text-align: center;
}

.progressBar .title text {
    color: #E72722;
}

.progressBar .helpProgress {
    margin: 80rpx 0 100rpx;
    height: 8rpx;
    position: relative;
    background: #dfdedf;
    border-radius: 20rpx;
    text-align: center;
}

.progressBar .helpProgress .item {
    position: absolute;
}

.progressBar .helpProgress .item.active .circle {
    background: #E72722;
}

.progressBar .helpProgress .item.active .coupon {
    color: #E72722;
}

.progressBar .helpProgress .bar {
    height: 8rpx;
    position: absolute;
    left: 0;
    top: 0;
    border-radius: 20rpx;
    background: #E72722;
}

.progressBar .helpProgress .item .circle {
    width: 12rpx;
    height: 12rpx;
    margin-left: -12rpx;
    position: absolute;
    background: #dfdedf;
    border: 4rpx solid #fff;
    border-radius: 50%;
    top: -6rpx;
}

.progressBar .helpProgress .item .coupon {
    position: absolute;
    color: #999;
    top: -45rpx;
    width: 100%;
    left: -50%;
    text-align: center;
    font-size: 22rpx;
}

.progressBar .helpProgress .item .num {
    position: absolute;
    color: #999;
    top: 20rpx;
    left: -50%;
    width: 100%;
    text-align: center;
    font-size: 22rpx;
}

.ms-container-invite .btn .button {
    border-radius: 100rpx;
    font-size: 30rpx;
    height: 85rpx;
    width: 100%;
    padding: 0;
    line-height: 85rpx;
    margin-bottom: 30rpx;
    text-align: center;
}

.ms-container-invite .btn .button::after {
    border: none;
}

.ms-container-invite .btn .gradient {
    background: linear-gradient(to right, #db2831, #fe8e6f);
    color: #fff;
    box-shadow: 0 0 20rpx #fe8e6f;
}

.ms-container-invite .btn .lineRed {
    background: #fff;
    border: 1px solid #E72722;
    color: #E72722;
}

.ms-lineHDMid .title {
    font-weight: 600;
    line-height: 50rpx;
}

.ms-lineHDMid .dec {
    font-size: 24rpx;
    color: #999;
}

.ms-lineHDMid .dec text {
    color: #E72722;
}

.ms-lineHDMid.lin {
    padding-bottom: 26rpx;
    border-bottom: 1px solid #efefef;
}

.countDown {
    margin-bottom: 20rpx;
}

.friendsHelp .list {
    padding: 20rpx 0;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    width: 110%;
    margin-left: -5%;
}

.friendsHelp .list .more {
    background: #f4f4f4;
    width: 80rpx;
    height: 80rpx;
    border: 1px solid #e7e7e7;
    font-size: 22rpx;
    display: flex;
    justify-content: center;
    padding-top: 10rpx;
    box-sizing: border-box;
    flex-direction: column;
    border-radius: 50%;
    text-align: center;
    margin-top: -30rpx;
}

.friendsHelp .list .item {
    display: flex;
    height: 150rpx;
    flex-direction: column;
    font-size: 26rpx;
    justify-content: center;
    align-items: center;
    width: 20%;
}

.friendsHelp .list .item image {
    background: #efefef;
    width: 80rpx;
    height: 80rpx;
    margin-bottom: 10rpx;
    border-radius: 50%;
}

.friendsHelp .hideMode {
    text-align: center;
}

.rulePopup .warp {
    max-height: 600rpx;
    overflow: auto;
}

.rulePopup .title {
    text-align: center;
    font-size: 32rpx;
    font-weight: 600;
    margin-bottom: 20rpx;
}

.rulePopup .warp .p {
    color: #555555;
    text-align: left;
    font-size: 24rpx;
    line-height: 40rpx;
    padding-bottom: 20rpx;
}


/*用户确认信息*/

.modal-msg {
    position: relative;
    padding: 30rpx;
}

.msg-top {
    position: absolute;
    padding: 10rpx;
    display: flex;
    flex-flow: column;
    top: 30rpx;
    left: 50%;
    transform: translate(-50%, -50%)
}

.msg-header-img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}

.msg-top-name {
    font-size: 36rpx;
    font-weight: 600;
}

.msg-infomation {
    font-size: 28rpx;
    margin: 30rpx 0;
    margin-top: 130rpx;
    text-align: center;
}

.msg-btn {
    display: flex;
    justify-content: space-around;
    align-items: center;
    flex-flow: row;
    font-size: 28rpx;
}

.fail-btn {
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 80rpx;
    border: 1rpx solid #cc2821;
    color: #cc2821;
}

.sure-btn {
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 80rpx;
    background: #cc2821;
    color: #fff;
}

.text-bottom {
    margin-bottom: 10rpx;
}


/* 返回首页 */

.both {
    position: fixed;
    z-index: 20;
    right: 20rpx;
    width: 80rpx;
    height: 180rpx;
    bottom: 100rpx;
    background-color: rgba(0, 0, 0, 0.6);
    border-radius: 40rpx;
}

.gd-index {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    height: 49%;
    position: relative;
}

.gd-index button {
    background: transparent;
    padding: 0;
    line-height: 30rpx;
}

.fix {
    margin-top: -2rpx;
}

.both .line {
    width: 44rpx;
    border-bottom: 1rpx solid #fff;
    margin: 0 auto;
    opacity: 0.5;
    height: 1rpx;
}

.gd-index image {
    width: 40rpx;
    height: 40rpx;
}

.gd-index view {
    width: 60rpx;
    font-size: 23rpx;
    color: #fff;
    text-align: center;
}

.ms-popup.getPhone .content {
    width: 590rpx;
    left: 0rpx;
}

.ms-popup.getPhone .content .dec {
    font-size: 30rpx;
    margin-top: 40rpx;
}

.ms-popup.getPhone .content .title {
    font-size: 32rpx;
    font-weight: 400;
}

.ms-popup.getPhone .btn .red {
    background: #E72722;
    color: #fff;
}

.ms-popup.getPhone .content .close {
    bottom: 325rpx;
    left: 535rpx;
    width: 30rpx;
    height: 30rpx;
    margin: 0;
}